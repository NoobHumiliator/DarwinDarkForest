<root>
    <styles>
       <include src="file://{resources}/styles/custom_game/top_menu.css" />
    </styles>

    <scripts>
        <include src="file://{resources}/scripts/custom_game/util.js" />
        <include src="file://{resources}/scripts/custom_game/top_menu.js" />
    </scripts>
    <Panel style="width:100%;height:100%;" hittest="false">

        <Panel id = "TopMenuPanel" class="">    
            <Panel class = "TopMenuIcon" id = "TopMenuIcon_Guide" onactivate = "OpenInstruction();" onmouseover="TipsOver('TopMenuIcon_Guide_message','TopMenuIcon_Guide')" onmouseout="TipsOut()">
            </Panel>
            <Panel class = "TopMenuIcon" id = "TopMenuIcon_Rank" onactivate = "OpenRank();" onmouseover="TipsOver('TopMenuIcon_Rank_message','TopMenuIcon_Rank')" onmouseout="TipsOut()">
            </Panel>
            <Panel class = "TopMenuIcon" id = "TopMenuIcon_Inventory" onactivate = "OpenInventory();" onmouseover="TipsOver('TopMenuIcon_Inventory_message','TopMenuIcon_Inventory')" onmouseout="TipsOut()">
            </Panel>
        </Panel>

    </Panel>
</root>